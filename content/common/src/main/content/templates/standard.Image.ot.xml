<?xml version="1.0" encoding="UTF-8"?>
<template-definition version="1.0" xmlns="http://www.polopoly.com/polopoly/cm/app/xml">
  <output-template name="standard.Image.ot" input-template="p.siteengine.ArticleOutputTemplate">
    <content-xml>
      <component group="output/modelKeyMode" name="selected">no-cache</component>
      <contentref group="output/renderer" name="selected">
        <contentid>
          <externalid>p.siteengine.VelocityFileRenderer</externalid>
        </contentid>
      </contentref>
      <contentref group="output/controller" name="selected">
        <contentid>
          <externalid>p.siteengine.JavaClassRenderController</externalid>
        </contentid>
      </contentref>
      <component group="output/controller/selected/renderControllerClass" name="value">com.atex.standard.image.ImageRenderController</component>
      <component group="output/renderer/selected/defaultVelocityViewFileName" name="value">/WEB-INF/velocity/standard.Image.vm</component>
      <component group="preview/controller" name="value">com.polopoly.cm.app.preview.PreviewControllerNotPreviewable</component>
    </content-xml>
  </output-template>
</template-definition>